<p id="notice"><%= notice %></p>

<h1>Listing Personas</h1>

<table>
  <thead>
    <tr>
      <th>Nombre1</th>
      <th>Nombre2</th>
      <th>Apellido1</th>
      <th>Apellido2</th>
      <th>Cedula</th>
      <th>Direccion</th>
      <th>Referencia</th>
      <th>Municipio</th>
      <th>Parroquia</th>
      <th>Telefono1</th>
      <th>Telefono2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @personas.each do |persona| %>
      <tr>
        <td><%= persona.nombre1 %></td>
        <td><%= persona.nombre2 %></td>
        <td><%= persona.apellido1 %></td>
        <td><%= persona.apellido2 %></td>
        <td><%= persona.cedula %></td>
        <td><%= persona.direccion %></td>
        <td><%= persona.referencia %></td>
        <td><%= persona.municipio %></td>
        <td><%= persona.parroquia %></td>
        <td><%= persona.telefono1 %></td>
        <td><%= persona.telefono2 %></td>
        <td><%= link_to 'Show', persona %></td>
        <td><%= link_to 'Edit', edit_persona_path(persona) %></td>
        <td><%= link_to 'Destroy', persona, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Persona', new_persona_path %>
