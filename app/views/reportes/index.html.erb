            </div>

            <div id="justified-tabs-4" aria-labelledby="ui-id-22" class="ui-tabs-panel ui-widget-content ui-corner-bottom" role="tabpanel" aria-expanded="false" aria-hidden="true" style="display: none;">
              get 'personas/:id/listado' => 'personas#listado'

